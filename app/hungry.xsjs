$.import("sflight.sflight.lib", "xs");var xs = $.sflight.sflight.lib.xs;var app = xs.create();app.get("/toggle", function(req, res) {	xs.queryAll("SELECT * FROM RDE_SAPPHIRE.RENOUNCEMEALS WHERE THISDAY = TO_DATE(NOW()) AND EMPLOYEE = '" + req.user.name + "'", [], function(err, objects) {		if(objects.length) {            xs.alter("DELETE FROM RDE_SAPPHIRE.RENOUNCEMEALS WHERE THISDAY = TO_DATE(NOW()) AND EMPLOYEE = '" + req.user.name + "'", function(err, affected) {                res.end("DELETED " + affected);            });        }        else {            xs.alter("INSERT INTO RDE_SAPPHIRE.RENOUNCEMEALS VALUES ('" + req.user.name + "', TO_DATE(NOW()))", function(err, affected) {                res.end("INSERTED " + affected);            });        }	});});app.listen();